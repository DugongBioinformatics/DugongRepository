<?xml version="1.0" encoding="UTF-8"?>
<component-descriptor xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="Descriptor Schema.xsd">
	<component name="BLAST Results Viewer"
		class="org.geworkbench.components.alignment.BlastViewComponent"
		version="1.0" 
		author="geWorkbench Development Team"
		authorURL="http://wiki.c2b2.columbia.edu/workbench/index.php/Home"
		tutorialURL="http://wiki.c2b2.columbia.edu/workbench/index.php/BLAST"
		toolURL="http://wiki.c2b2.columbia.edu/workbench/index.php/Home"
		description="The BLAST Alignment Results component parses and displays
 the results of sequence similarity searches which were run on the
 NCBI BLAST service.  Sequence hits can be returned to the Project
 for further analysis."
		mustAccept="false" 
		category="Visualizer, Sequence" loadByDefault="true">
		<required-component
			class="org.geworkbench.components.alignment.blast.BlastAnalysis" />
		<license><![CDATA[
			<html><head><meta http-equiv=Content-Type content="text/html; charset=windows-1252"></head>
				<body>----</body>
			</html>
		]]></license>

	</component>
	<plugin id="BlastViewComponent" name="Alignment Results"
		class="org.geworkbench.components.alignment.BlastViewComponent"
		source="alignment">
		<gui-area name="VisualArea" />
	</plugin>
</component-descriptor>
