			<html><head><meta http-equiv=Content-Type content="text/html; charset=windows-1252"></head>
				<body>----</body>
			</html>
		]]></license>

	</component>

	<plugin id="CellularNetworkKnowledgeWidget" name="Cellular Network Knowledge Base"
		class="org.geworkbench.components.interactions.cellularnetwork.CellularNetworkKnowledgeWidget"
		source="interactions">
		<gui-area name="VisualArea" />
	</plugin>

